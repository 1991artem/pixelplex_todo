{
  "name": "pixelplex",
  "version": "1.0.0",
  "description": "NodeJS PixelPlex task",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start": "tsc -w",
    "build": "npx tsc",
    "launch": "ts-node --transpile-only ./src/app.ts",
    "launch:dev": "ts-node-dev --files ./src/app.ts",
    "audit": "npx audit-ci --config audit-ci.json",
    "lint": "npx eslint ./src",
    "lint:fix": "npx eslint ./src --fix"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1991artem/pixelplex_todo.git"
  },
  "keywords": [
    "PixelPlex",
    "NodeJS"
  ],
  "author": "Artem Puzik <artem.puzik.it@gmail.com>",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/1991artem/pixelplex_todo/issues"
  },
  "homepage": "https://github.com/1991artem/pixelplex_todo#readme",
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/node": "^18.11.9",
    "@typescript-eslint/eslint-plugin": "^5.44.0",
    "@typescript-eslint/parser": "^5.44.0",
    "audit-ci": "^6.3.0",
    "eslint": "^8.28.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.2",
    "eslint-plugin-import": "^2.26.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "typescript": "^4.9.3"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-validator": "^6.14.2"
  }
}
